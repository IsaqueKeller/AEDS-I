#include <iostream>
#include <math.h>
#include <string>

using namespace std;


  	int dobroDoNumero (int x) {
	  
		int dobro = 0;
		dobro = 2*x; 
		return dobro;
  
  }
  int main(void) {
	int resp = 0;
  resp = dobroDoNumero(3);
   cout << resp <<  endl;
   
  resp = dobroDoNumero(12);
   cout << resp << endl;
	}
  	
 
