#include <iostream>
using namespace std;

int main () {
	int grade = 0;
		
		cout << "Type in a grade: " << endl;
		cin >> grade;
		if (grade >= 0 and grade <= 10) {
			cout << "OK" << endl;
		}
else {

	do {
	cout << "Error" << endl;
	cout << "Type in a grade: " << endl;
		cin >> grade;
		
	} while (grade < 0 or grade > 10); 

}
}
