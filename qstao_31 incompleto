// 
#include <iostream>
using namespace std;

void age_function (int age) {
	if (5 <= age <= 7) {
		cout << age << " - Infant A" << endl;
	}
	else if (8 <= age <= 10) {
		cout << age << " - Infant B" << endl; 
	}
	else if (11 <= age <= 13) {
		cout << age << " - Juvenile A" << endl;
	}
	else if (14 <= age <= 17) {
		
	}
}
