//Escrever um algoritmo que le um conjunto de 4 valores i, a, b, c, onde i e um valor inteiro e positivo e a, b, c, sao quaisquer valores reais e os escreva
#include <iostream>
using namespace std;

void numbers (int i , float a , float b , float c) {
	if (i == 1) { //ordem crescente
		if (a > b and a > c and b > c) {		
		
		cout << c << b << a << endl;
			}
			
			else if (b > a and b > c and c > a){
				cout << a << c << b << endl;
			}
			else if (a > c and a > b and c > b){
				cout << b << c << a << endl; 
			}
			else if ()
		}
	
	else if (i == 2) {
		if (a > b and c > b and a > c) {
			cout << a << c << b << endl;
		}
		else if ()
	}
	else if (i == 3) {
		if (a > b and b > c) {
			cout << b << a << c << endl;
		}
		else if (b > a and a > c) {
			cout << a << b << c << endl;
		}
		else if (c > a and a > b) {
			cout << a << c << b << endl;
		}
	}
}
 


int main (void) {
	int v1 = 0;
	float v2 = 0 , v3 = 0 , v4 = 0;
	
	cin >> v1;
	cin >> v2;	
	cin >> v3;
	cin >> v4;
numbers (v1, v2, v3, v4);	
}
