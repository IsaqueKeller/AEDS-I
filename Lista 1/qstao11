#include <iostream>
#include <math.h>
using namespace std;

// f(x , y) = x² + y² - 6xy - 1 

float funcaogeral (float x , float y) {
	float xquadrado = pow (x , 2);
	float yquadrado = pow (y , 2); 
	
	return xquadrado + yquadrado - 6*x*y - 1;
}

int main(void) {
	float v1 = 0;
	float v2 = 0;
	
	cout << "Insira um valor para x: " << endl;
	cin >> v1;
	
	cout << "Insira um valor para y: " << endl;
	cin >> v2;
	
	cout << "O resultado eh: " << funcaogeral(v1 , v2) << endl; 
	
}
