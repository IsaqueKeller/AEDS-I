#include <iostream>
using namespace std;

int function (int n1 , int n2 , int n3) {
char three_equal = 'C';
char two_equal = 'A';

if (n2 < n1 && n3 < n1) {	
	return n1;
}	
else {
	return 0;
}
if (n1 == n2 && n2 == n3) {
	return three_equal;
}			
else if (n1 == n2 or n2 == n3 or n1 == n3) {
	return two_equal;
}
}
 


int main () {
int v1 = 0 , v2 = 0 , v3 = 0;	

cout << "Inform three numbers: " << endl;
cin >> v1;
cin >> v2;
cin >> v3;

int result = function (v1 , v2 , v3);
if (result == 'C') {
	cout << "The numbers are equal." << endl;
}
else if (result == 'A'){
cout << "Two numbers are equal" << result << endl;	
}
}
